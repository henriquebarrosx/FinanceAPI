{
	"compilerOptions": {
		"outDir": "dist",
		"target": "es2016",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"#/*": [
				"./src/*"
			]
		}
	},
	"exclude": [
		"node_modules"
	]
}